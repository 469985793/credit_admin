[
  {
    "path": "/",
    "name": "index",
    "component": "/Index",
    "meta": { "title": "急我贷", "icon": "example" },
    "redirect": "/dashboard",
    "children": [
      {
        "path": "/dashboard",
        "name": "dashboard",
        "component": "/dashboard/Index",
        "meta": { "title": "控制面板", "icon": "example" }
      },
      {
        "path": "/authority",
        "name": "authority",
        "component": "/authority/Index",
        "meta": { "title": "权限管理", "icon": "example" }
      },
      {
        "path": "/creditQuery",
        "name": "creditQuery",
        "component": "/creditQuery/Index",
        "meta": { "title": "信用查询", "icon": "example" }
      },
      {
        "path": "customer",
        "name": "customer",
        "component": "/customer/Index",
        "meta": { "title": "客户中心", "icon": "example" },
        "redirect": "/customer/list",
        "children": [
          {
            "path": "list",
            "name": "customer",
            "component": "/customer/List",
            "meta": { "title": "客户列表", "icon": "example" }
          },
          {
            "path": "detail/:customerId",
            "name": "customerDetail",
            "component": "/customer/detail/Index",
            "meta": { "title": "客户详情", "icon": "example" },
            "children": [
              {
                "path": "baseInfo",
                "name": "customerDetailBaseInfo",
                "component": "/customer/detail/BaseInfo",
                "meta": { "title": "基本信息", "icon": "example" }
              },
              {
                "path": "operator",
                "name": "customerDetailOperator",
                "component": "/customer/detail/operator/Index",
                "meta": { "title": "运营商认证", "icon": "example" }
              },
              {
                "path": "alipay",
                "name": "customerDetailAlipay",
                "component": "/customer/detail/alipay/Index",
                "meta": { "title": "支付宝认证", "icon": "example" }
              },
              {
                "path": "alimarket",
                "name": "customerDetailAlimarket",
                "component": "/customer/detail/aliMarket/Index",
                "meta": { "title": "淘宝认证", "icon": "example" }
              },
              {
                "path": "contact",
                "name": "customerDetailContact",
                "component": "/customer/detail/Contact",
                "meta": { "title": "通讯录认证", "icon": "example" }
              },
              {
                "path": "credit",
                "name": "customerDetailCredit",
                "component": "/customer/detail/credit/Index",
                "meta": { "title": "征信报告", "icon": "example" }
              },
              {
                "path": "insurance",
                "name": "customerDetailInsurance",
                "component": "/customer/detail/insurance/Index",
                "meta": { "title": "社保", "icon": "example" }
              },
              {
                "path": "housingFund",
                "name": "customerDetailHousingFund",
                "component": "/customer/detail/housingFund/Index",
                "meta": { "title": "公积金", "icon": "example" }
              },
              {
                "path": "applyRecord",
                "name": "customerDetailApplyRecord",
                "component": "/customer/detail/ApplyRecord",
                "meta": { "title": "申请记录", "icon": "example" }
              }
            ]
          }
        ]
      },
      {
        "path": "/process",
        "name": "process",
        "component": "/process/Index",
        "meta": { "title": "进度管理", "icon": "example" },
        "redirect": "/process/waitVerify",
        "children": [
          {
            "path": "waitVerify",
            "name": "processWaitVerify",
            "component": "/process/WaitVerify",
            "meta": { "title": "待审核", "icon": "example" }
          },
          {
            "path": "verify",
            "name": "processverify",
            "component": "/process/Verify",
            "meta": { "title": "审核中", "icon": "example" }
          },
          {
            "path": "refuse",
            "name": "processRefuse",
            "component": "/process/Refuse",
            "meta": { "title": "拒绝受理", "icon": "example" }
          },
          {
            "path": "waitLoan",
            "name": "processWaitLoan",
            "component": "/process/WaitLoan",
            "meta": { "title": "待放款", "icon": "example" }
          },
          {
            "path": "loan",
            "name": "processLoan",
            "component": "/process/Loan",
            "meta": { "title": "已放款", "icon": "example" }
          }
        ]
      },
      {
