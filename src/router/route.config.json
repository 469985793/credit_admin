            "meta": { "title": "还款", "icon": "example" }
          },
          {
            "path": "acquitt",
            "name": "postLoanAcquitt",
            "component": "/postLoan/Acquitt",
            "meta": { "title": "已结清", "icon": "example" }
          },
          {
            "path": "overdue",
            "name": "postLoanOverdue",
            "component": "/postLoan/Overdue",
            "meta": { "title": "逾期", "icon": "example" }
          }
        ]
      }
    ]
  },
  {
    "path": "/login",
    "name": "login",
    "component": "/login/Index",
    "meta": { "title": "登录", "icon": "example" }
  }
]

